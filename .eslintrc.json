{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "no-unused-vars": "off",
        "no-use-before-define": "off",
        "no-undef": "off",
        "no-plusplus": "off"
    }
}
